{
    "version": 2,
    "build": {
      "env": {
        "NODE_ENV": "production"
      }
    },
    "routes": [
      {
        "src": "/static/(.*)",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }